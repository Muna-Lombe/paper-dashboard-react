{
  "name": "paper-dashboard-react",
  "version": "1.3.2",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "array-move": "^4.0.0",
    "axios": "^1.7.5",
    "bootstrap": "4.6.2",
    "browser-cookie": "^1.2.1",
    "chart.js": "3.9.1",
    "concurrently": "9.1.2",
    "framer-motion": "^11.3.30",
    "html2canvas": "^1.4.1",
    "perfect-scrollbar": "1.5.5",
    "popmotion": "^11.0.5",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "3.3.0",
    "react-dom": "18.2.0",
    "react-notification-alert": "0.0.13",
    "react-redux": "^9.1.2",
    "react-router-dom": "6.11.1",
    "react-scripts": "5.0.1",
    "react-use-websocket": "^4.8.1",
    "reactstrap": "8.10.0",
    "sass": "1.62.1"
  },
  "scripts": {
    "add-basename": "node addBasename.js",
    "remove-basename": "node removeBasename.js",
    "predeploy": "npm run add-basename && npm run build && npm run remove-basename",
    "deploy": "npm run predeploy && gh-pages -d build",
    "start": "npm run install:clean-slate && concurrently \"react-scripts start\" \"node server/index.js\"",
    "build": "npm run install:clean-slate && concurrently \"react-scripts build\" \"node server/index.js\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server-dev": "nodemon -p 5000 server/index.js",
    "server-start": "node server/index.js",
    "dev": "concurrently \"react-scripts start\" \"nodemon server/index.js\"",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
    "install:clean-server": "cd server/ && rm -rf node_modules/ && rm -rf package-lock.json && npm install && cd ..",
    "install:clean-slate": "npm run install:clean && npm run install:clean-server",
    "compile-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css",
    "minify-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css --source-map true",
    "lint": "eslint .",
    "clean": "npm prune --production && rm -rf build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "optionalDependencies": {
    "typescript": "5.0.4"
  },
  "overrides": {
    "svgo": "3.0.2",
    "fsevents": "2.3.2",
    "chokidar": "3.5.3",
    "@babel/core": "7.18.6"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1"
  },
  "homepage": "https://muna-lombe.github.io/paper-dashboard-react"
}
